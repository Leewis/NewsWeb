  "PageLevelConfigs": [
    {
      "Url": "rss.htm",
      "HtmlSelectors": [
        {
          "PropertyName": "Name",
          "HtmlSelector": ".highlight ul li a"
        }
      ],
      "NextLevelUrlSelector": {
        "HtmlSelector": ".highlight ul li a"
      },
      "ProcessorTypeName": "BaseSimpleRssCategoryProcessor",
      "Level": 0,
      "AdditionalData": "",
